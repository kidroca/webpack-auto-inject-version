{
  "babelrc": false,
  "presets": [
    "es2015",
    "stage-2"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src/"],
       "alias": {
         "config": "./config",
         "core": "./core",
         "components": "./components"
       }
    }],
    "transform-class-properties",
    "transform-runtime"
  ]
}
